<?xml version="1.0" encoding="utf-8"?>
<!-- This is a parameters and settings customising file to process BIDS multimodal test dataset ds114_test1 (https://googledrive.com/host/0B2JWN60ZLkgkMEw4bW5VUUpSdFU/ds114_test1.tar) -->
<aap>
    <options>
        <!--wheretoprocess>matlab_pct</wheretoprocess>
        <aaparallel>
            <numberofworkers>4</numberofworkers>
        </aaparallel-->
        <NIFTI4D>1</NIFTI4D>
        
        <autoidentifystructural_choosefirst>1</autoidentifystructural_choosefirst>
    </options>

    <acq_details>
        <numdummies>0</numdummies>
        <input>
            <isBIDS>1</isBIDS>
            <combinemultiple>1</combinemultiple>
        </input>
    </acq_details>
        
    <tasksettings>
        <aamod_norm8>
            <vox>1</vox>
        </aamod_norm8>
        <aamod_slicetiming>
            <autodetectSO>1</autodetectSO>
            <refslice>16</refslice>
        </aamod_slicetiming>
        <aamod_norm_write>
            <vox>3 3 3</vox>
        </aamod_norm_write>
        <aamod_smooth>
            <FWHM>5</FWHM>
        </aamod_smooth>
        <aamod_firstlevel_model>
            <includemovementpars>1</includemovementpars>
        </aamod_firstlevel_model>
    </tasksettings>
   
</aap>
